<?xml version="1.0" encoding="UTF-8"?>
<templateSet group="android">
  <template name="hdlr" value="new android.os.Handler() {&#10;    @Override&#10;    public void handleMessage(android.os.Message msg) {&#10;        $END$&#10;    }&#10;};" description="Creates a new handler object." toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_DECLARATION" value="true" />
      <option name="JAVA_COMMENT" value="true" />
      <option name="JAVA_STRING" value="true" />
      <option name="COMPLETION" value="true" />
    </context>
  </template>
  <template name="err" value="android.util.Log.e(&quot;$CLASS$&quot;, &quot;$MESSAGE$&quot;);" description="Logs an error message." toReformat="false" toShortenFQNames="true">
